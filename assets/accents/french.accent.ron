                        "spicy": {"Any": [
                            {"Literal": "épicé"},
                            {"Literal": "épicée"},
                        ]},
                        "yes": {"Literal": "oui"},
                        "no": {"Literal": "non"},
                        "why": {"Literal": "pourquoi"},
                        "what'?s": {"Literal": "quel est"},
